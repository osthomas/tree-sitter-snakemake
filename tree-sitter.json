{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "snakemake",
      "camelcase": "Snakemake",
      "title": "snakemake",
      "scope": "source.snakemake",
      "file-types": [
        "Snakefile",
        "smk"
      ],
      "injection-regex": "^snakemake$",
      "class-name": "TreeSitterSnakemake",
      "highlights": "queries/snakemake/highlights.scm"
    }
  ],
  "metadata": {
    "version": "2.0.0",
    "license": "MIT",
    "description": "snakemake grammar for tree-sitter, based on tree-sitter-python",
    "authors": [
      {
        "name": "Oliver Thomas"
      }
    ],
    "links": {
      "repository": "https://github.com/osthomas/tree-sitter-snakemake"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
