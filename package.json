{
  "devDependencies": {
    "tree-sitter-cli": "^0.21.0",
    "tree-sitter-python": "^0.21.0"
  },
  "name": "tree-sitter-snakemake",
  "version": "1.0.0",
  "description": "snakemake grammar for tree-sitter, based on tree-sitter-python",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter test",
    "build": "tree-sitter generate --no-bindings"
  },
  "keywords": [
    "parser",
    "snakemake",
    "python"
  ],
  "author": "Oliver Thomas",
  "license": "MIT",
  "tree-sitter": [
    {
      "scope": "source.snakemake",
      "file-types": [
        "Snakefile",
        "smk"
      ],
      "highlights": "queries/snakemake/highlights.scm"
    }
  ],
  "dependencies": {
    "nan": "^2.19.0"
  }
}
